# ===== Models (IEBIN v1 and raw HF dumps) =====
# Keep the directory structure and small JSON metadata (IEBIN + vocab),
# ignore large weight blobs and raw checkpoints/shards.
models/**/
!models/**/.gitkeep
# IEBIN weights (usually huge)
models/**/*.bin
# HF raw formats
models/**/*.safetensors
models/**/model.safetensors.index.json
models/**/pytorch_model.bin
models/**/pytorch_model.bin.index.json
models/**/*.ckpt
models/**/*.pt
models/**/*.onnx
models/**/SHA256SUMS
# Allow IEBIN metadata & vocab (small JSONs)
!models/**/model.ie.json
!models/**/vocab.json

