        <argsstring>(const float *W, size_t rows, size_t cols, size_t blk_k, const char *cache_path, ie_wblocked_desc_t *out)</argsstring>
        <name>ie_layout_build_blockedK</name>
        <param>
          <type><ref refid="ie__kernels__cuda_8cu_1af00402674d56c833c92c62df1cef1e84" kindref="member">const</ref> <ref refid="ie__kernels__cuda_8cu_1af00402674d56c833c92c62df1cef1e84" kindref="member">float</ref> *</type>
